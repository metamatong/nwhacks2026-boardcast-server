general: {
  configs_folder = "/opt/janus/etc/janus"
  plugins_folder = "/opt/janus/lib/janus/plugins"
  transports_folder = "/opt/janus/lib/janus/transports"
  loggers_folder = "/opt/janus/lib/janus/loggers"
  log_to_stdout = true
  log_timestamp = true
  log_level = 4
}

media: {
  rtp_port_range = "10000-10200"
}

nat: {
  nat_1_1_mapping = "__JANUS_PUBLIC_IP__"
}

cert_pem = "/janus/certs/janus.crt"
cert_key = "/janus/certs/janus.key"
